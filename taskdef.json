{
    "executionRoleArn": "arn:aws:iam::969829910614:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "TaskOnly-TaskDefinition",
            "image": "969829910614.dkr.ecr.us-east-2.amazonaws.com/ecs-taskonly-repo:codepipeline-app",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "TaskOnlyTaskDefinition"
}